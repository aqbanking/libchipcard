<?xml?>

<gwbuild>

  <target type="ConvenienceLibrary" name="cards_processorcard" >

    <includes type="c" >
      $(gwenhywfar_cflags)
      -I$(topsrcdir)/src
      -I$(topbuilddir)/src
      -I$(topsrcdir)
      -I$(topbuilddir)
    </includes>

    <includes type="tm2" >
      --include=$(gwenhywfar_dir)/share/gwenhywfar/typemaker2/c
      --include=$(builddir)
    </includes>

  
    <define name="BUILDING_CHIPCARD" />
    
    <setVar name="local/cflags">$(visibility_cflags)</setVar>

    <setVar name="tm2flags" >
      --api=CHIPCARD_API
    </setVar>

  
    <setVar name="local/typefiles" >
    </setVar>
  
    <setVar name="local/built_sources" >
    </setVar>
  
    <setVar name="local/built_headers_pub">
    </setVar>
  
  
    <setVar name="local/built_headers_priv" >
    </setVar>
  
  
    <headers dist="true" install="$(includedir)/libchipcard6/libchipcard/cards/processorcard" >
      $(local/built_headers_pub)

      processorcard.h
    </headers>


    <headers dist="true" >
      $(local/built_headers_priv)

      processorcard_p.h
    </headers>
  
  
    <sources>
      $(local/typefiles)

      processorcard.c
    </sources>


    <data dist="true" install="$(datadir)/chipcard/apps">
      processorcard-app.xml
    </data>

    <data dist="true" install="$(datadir)/chipcard/cards">
      processorcard-card.xml
    </data>

    <extradist>
    </extradist>


    <useTargets>
    </useTargets>

    <subdirs>
    </subdirs>

  </target>
  
</gwbuild>
