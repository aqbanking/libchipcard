    </headers>


    <headers dist="true" >
      kvkcard_p.h
    </headers>
  
  
    <sources>
      kvkcard.c
    </sources>


    <data dist="true" install="$(datadir)/chipcard/apps">
      kvkcard-app.xml
    </data>

    <data dist="true" install="$(datadir)/chipcard/cards">
      kvkcard-card.xml
    </data>

  </target>
  
</gwbuild>
