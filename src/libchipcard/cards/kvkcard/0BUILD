      -I$(topbuilddir)/src
      -I$(topsrcdir)
      -I$(topbuilddir)
    </includes>

    <define name="BUILDING_CHIPCARD" />
    
    <setVar name="local/cflags">$(visibility_cflags)</setVar>

    <headers dist="true" install="$(includedir)/libchipcard6/libchipcard/cards/kvkcard" >
