      -I$(topsrcdir)/src
      -I$(topbuilddir)/src
      -I$(topsrcdir)
      -I$(topbuilddir)
    </includes>
  
    <setVar name="local/cxxflags">$(visibility_cflags)</setVar>

  
    <headers dist="true" >
      global.h
    </headers>
  
  
    <sources>
      cardcommander.cpp
    </sources>

    <useTargets>
      chipcard
    </useTargets>

    <libraries>
      $(gwenhywfar_libs)
    </libraries>


  </target>


</gwbuild>
