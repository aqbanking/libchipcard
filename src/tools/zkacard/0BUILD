  <target type="Program" name="zkacard" install="$(bindir)" >

    <includes type="c" >
      $(gwenhywfar_cflags)
      -I$(topsrcdir)/src
      -I$(topbuilddir)/src
      -I$(topsrcdir)
      -I$(topbuilddir)
    </includes>
  
    <setVar name="local/cflags">$(visibility_cflags)</setVar>

  
    <headers dist="true" >
      global.h
    </headers>
  
  
    <sources>
      main.c
      getpublickey.c
      shownotepad.c
      resetptc.c
    </sources>

    <useTargets>
      chipcard
    </useTargets>

    <libraries>
      $(gwenhywfar_libs)
    </libraries>


  </target>


</gwbuild>
