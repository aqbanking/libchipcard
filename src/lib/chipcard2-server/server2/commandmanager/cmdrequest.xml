<typedefs>

  <type id="GWEN_TIME" >
    <functions>
      <function type="todb" name="GWEN_Time_toDb" />
      <function type="fromdb" name="GWEN_Time_fromDb" />
      <function type="dup" name="GWEN_Time_dup" />
      <function type="free" name="GWEN_Time_free" />
    </functions>
  </type>

  <type id="GWEN_XMLNODE" >
    <functions>
      <function type="free" name="GWEN_XMLNode_free" />
    </functions>
  </type>

  <type id="GWEN_TYPE_UINT32" dbtype="int" />

</typedefs>


<types>
  <type id="LCCMD_CMDREQUEST" 
        prefix="LCCMD_CmdRequest" 
        class="CmdRequest"
        access="lib"
        filename="cmdrequest"
        list="lib" >
    <headers>
      <header type="sys" >gwenhywfar/types.h</header>
      <header type="sys" >gwenhywfar/gwentime.h</header>
      <header type="sys" >gwenhywfar/xml.h</header>
    </headers>
    
    <elem name="requestId" type="GWEN_TYPE_UINT32" />
    <elem name="requestTime" type="GWEN_TIME" ptr="1" />
    <elem name="cmdNode" type="GWEN_XMLNODE" ptr="1" 
          copy="0" volatile="1" const="0" takeOver="0" />
  </type>
  
  
  
</types>
