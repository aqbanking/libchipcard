<!-- contains all readers again since cyberjack driver 2.0.10 -->

<driver name="cyberjack_ctapi" type="ctapi">
  <short>Driver for the Reiner-SCT Cyberjack</short>
  <manufacturer>
    <name>Reiner SCT</name>
    <url>http://www.reiner-sct.de/</url>
  </manufacturer>

  <lib>
    <names>
      <name>libctapi-cyberjack.so</name>
      <name>libctapi-cyberjack.so.*</name>
    </names>
    
    <locations>
      <loc>/lib</loc>
      <loc>/usr/lib</loc>
      <loc>/usr/local/lib</loc>
      <loc>/lib64</loc>
      <loc>/usr/lib64</loc>
      <loc>/usr/local/lib64</loc>
    </locations>

    <downloads>
      <url>http://support.reiner-sct.de/</url>
      <filename>ctapi-cyberjack-3.0.5.tar.bz2</filename>
    </downloads>
  </lib>

  <readers>

    <!-- Old Cyberjack or Cyberjack E-com -->
    <reader name="cyberjack" 
            bustype="UsbRaw"
            vendor="0x0c4b" product="0x0100"
            addressType="port" >
    <!-- This is for newer Cyberjack drivers
    <reader name="cyberjack" 
            bustype="UsbRaw"
            addressType="devicePath"
            devicePathTmpl="usb:$(vendorId:04x)/$(productId:04x):libusb:$(busName):$(deviceName)"
            vendor="0x0c4b" product="0x0100" >
    -->
      <short>Cyberjack pinpad or ecom</short>

      <flags>
        <flag>keypad</flag>
      </flags>

      <autoport>
        <mode>pos</mode>
        <busorder>UsbRaw</busorder>
        <sortkey>vendorId</sortKey>
        <offset>1</offset>
      </autoport>

      <ports>
        <port value="1">Device0</port>
        <port value="2">Device1</port>
        <port value="3">Device2</port>
        <port value="4">Device3</port>
        <port value="5">Device4</port>
        <port value="6">Device5</port>
        <port value="7">Device6</port>
        <port value="8">Device7</port>
      </ports>

      <image>cyberjack_pinpad.jpg</image>
    </reader>


    <!-- New Cyberjack -->
    <reader name="cyberjack2" 
            ctn="1" bustype="UsbRaw"
            vendor="0x0c4b" product="0x0300">

    <!-- This is for newer Cyberjack drivers
    <reader name="cyberjack2" 
            ctn="1" bustype="UsbRaw"
            addressType="devicePath"
            devicePathTmpl="usb:$(vendorId:04x)/$(productId:04x):libusb:$(busName):$(deviceName)"
            vendor="0x0c4b" product="0x0300">
    -->
    
      <short>Cyberjack pinpad</short>
      <flags>
        <flag>keypad</flag>
      </flags>
      
      <autoport>
        <mode>pos</mode>
        <busorder>UsbRaw</busorder>
        <sortkey>vendorId</sortKey>
        <offset>1</offset>
      </autoport>
      
      <ports>
        <port value="1">Device0</port>
        <port value="2">Device1</port>
        <port value="3">Device2</port>
        <port value="4">Device3</port>
        <port value="5">Device4</port>
        <port value="6">Device5</port>
        <port value="7">Device6</port>
        <port value="8">Device7</port>
      </ports>

      <image>cyberjack_pinpad.jpg</image>

    </reader>

    <!-- New Ecom -->
    <reader name="cyberjack3" 
            ctn="1" bustype="UsbRaw"
            vendor="0x0c4b" product="0x0400">

    <!-- This is for newer Cyberjack drivers
    <reader name="cyberjack2" 
            ctn="1" bustype="UsbRaw"
            addressType="devicePath"
            devicePathTmpl="usb:$(vendorId:04x)/$(productId:04x):libusb:$(busName):$(deviceName)"
            vendor="0x0c4b" product="0x0300">
    -->
    
      <short>Cyberjack Ecom (a)</short>
      <flags>
        <flag>keypad</flag>
        <flag>display</flag>
      </flags>
      
      <autoport>
        <mode>pos</mode>
        <busorder>UsbRaw</busorder>
        <sortkey>vendorId</sortKey>
        <offset>1</offset>
      </autoport>
      
      <ports>
        <port value="1">Device0</port>
        <port value="2">Device1</port>
        <port value="3">Device2</port>
        <port value="4">Device3</port>
        <port value="5">Device4</port>
        <port value="6">Device5</port>
        <port value="7">Device6</port>
        <port value="8">Device7</port>
      </ports>

      <image>cyberjack_ecoma.jpg</image>

    </reader>

    <!-- Pinpad (a) with new firmware -->
    <reader name="cyberjack4" 
            ctn="1" bustype="UsbRaw"
            vendor="0x0c4b" product="0x0401">

    <!-- This is for newer Cyberjack drivers
    <reader name="cyberjack2" 
            ctn="1" bustype="UsbRaw"
            addressType="devicePath"
            devicePathTmpl="usb:$(vendorId:04x)/$(productId:04x):libusb:$(busName):$(deviceName)"
            vendor="0x0c4b" product="0x0300">
    -->
    
      <short>Cyberjack Pinpad (a2)</short>
      <flags>
        <flag>keypad</flag>
      </flags>
      
      <autoport>
        <mode>pos</mode>
        <busorder>UsbRaw</busorder>
        <sortkey>vendorId</sortKey>
        <offset>1</offset>
      </autoport>
      
      <ports>
        <port value="1">Device0</port>
        <port value="2">Device1</port>
        <port value="3">Device2</port>
        <port value="4">Device3</port>
        <port value="5">Device4</port>
        <port value="6">Device5</port>
        <port value="7">Device6</port>
        <port value="8">Device7</port>
      </ports>

      <image>cyberjack_pinpad_a2.jpg</image>

    </reader>

  </readers>


  <!-- MacOS specific part -->

  <lib osname="osx">
    <names>
      <name>CyberJack_CTAPI</name>
    </names>

    <locations>
      <loc>/Library/Frameworks/CyberJack_CTAPI.framework/Versions/Current</loc>
      <loc>/lib</loc>
      <loc>/usr/lib</loc>
      <loc>/usr/local/lib</loc>
    </locations>
    
    <downloads>
      <url>http://support.reiner-sct.de/downloads/LINUX/V2.0.12/</url>
      <filename>ctapi-cyberjack-3.0.5.tar.bz2</filename>
    </downloads>
  </lib>

</driver>

