
<driver name="cyberjack_ctapi" type="ctapi">
  <short>Driver for the Reiner-SCT Cyberjack</short>
  <manufacturer>
    <name>Reiner SCT</name>
    <url>http://www.reiner-sct.de/</url>
  </manufacturer>

  <lib>
    <names>
      <name>libctapi-cyberjack1.so</name>
    </names>
    
    <locations>
      <loc>/lib</loc>
      <loc>/usr/lib</loc>
      <loc>/usr/local/lib</loc>
    </locations>
    
    <downloads>
      <url>http://support.reiner-sct.de/downloads/LINUX/V1.0.0/</url>
      <filename>ctapi-cyberjack-1.0.0.tar.gz</filename>
    </downloads>
  </lib>

  <readers>

    <reader name="cyberjack" com="USBSerial" vendor="0x0c4b" product="0x0100">
      <short>Cyberjack pinpad or ecom</short>
      <ports>
        <port value="1">USB0</port>
        <port value="2">USB1</port>
        <port value="3">USB2</port>
        <port value="4">USB3</port>
      </ports>
      <flags>
        <flag>keypad</flag>
      </flags>
      <image>cyberjack_pinpad.jpg</image>
    </reader>

  </readers>


</driver>

