      <loc>/usr/local/lib/pcsc/drivers/ifd-ccid.bundle/Contents/Linux</loc>
    </locations>
    
    <downloads>
      <url>http://alioth.debian.org/project/showfiles.php?group_id=1225&release_id=303</url>
      <filename></filename>
    </downloads>
  </lib>

