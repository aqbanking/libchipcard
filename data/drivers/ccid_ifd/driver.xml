
<driver name="ccid_ifd" type="ccid">
  <short>Generic Driver for CCID Devices</short>
  <manufacturer>
    <name>Generic</name>
    <url>http://pcsclite.alioth.debian.org/ccid.html</url>
  </manufacturer>

  <lib>
    <names>
      <name>libccid.so</name>
      <name>libccid.so.*</name>
      <name>libccid.so.0.9.?</name>
    </names>
    
    <locations>
      <loc>/usr/local/lib/chipcard2-server/lowlevel/ifd/ifd-ccid.bundle/Contents/Linux</loc>
