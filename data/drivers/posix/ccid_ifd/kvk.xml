<cards>
  <card name="kvk" >

    <commands>
      <!-- Select Master File -->
      <command name="SelectMF" driver="ccid_ifd">
       <send>
         <APDU lr="0" >
           <ELEM type="dword">0x00a40000</ELEM>
         </APDU>
         <DATA>
           <ELEM type="word">0x3f00</ELEM>
         </DATA>
       </send>
       <results>
         <result sw1="0x6a" sw2="0x82" type="error">File not found</result>
       </results>
      </command>

      <!-- Select Master File for Kobil Advanced -->
      <command name="SelectMF"
               driver="ccid_ifd" 
               reader="ko_kaan_advanced_usb">
       <!-- empty command -->
      </command>

    </commands>
  </card>
</cards>

