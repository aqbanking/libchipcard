<cards>
  <card name="kvk" >

    <commands>
      <!-- Select Master File -->
      <command name="SelectMF" driver="cyberjack_pcsc" >
      	<send>
	  			<APDU>
  	    		<ELEM type="dword">0x00a40000</ELEM>
	  			</APDU>
	  			<DATA>
	    			<ELEM type="word">0x3f00</ELEM>
	  			</DATA>
				</send>
				<results>
      		<result sw1="0x6a" sw2="0x82" type="error">File not found</result>
				</results>
    	</command>
    </commands>
  </card>
</cards>

