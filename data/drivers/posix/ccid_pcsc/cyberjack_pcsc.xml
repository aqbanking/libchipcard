
<driver name="ccid_pcsc" type="ifd">
  <short>Generic CCID Driver</short>
  <manufacturer>
    <name>Ludovic Rousseau</name>
    <url>http://pcsclite.alioth.debian.org/ccid.html</url>
  </manufacturer>

  <readers>

    <reader name="ko_kaan_base_usb" busType="UsbRaw"
            devicePathTmpl="KOBIL KAAN Advanced (*" >
      <short>Kaan Advanced (USB)</short>

      <flags>
        <!-- <flag>keypad</flag> -->
        <flag>keypad</flag>
      </flags>
    </reader>

    <reader name="ko_kaan_base_usb" busType="UsbRaw"
            devicePathTmpl="KOBIL KAAN Advanced (*" >
      <short>Kaan Advanced (USB)</short>

      <flags>
        <!-- <flag>keypad</flag> -->
        <flag>keypad</flag>
      </flags>
    </reader>

    <reader name="ccid_cherry_xx44" busType="UsbRaw"
            devicePathTmpl="Cherry XX44 *" >
      <short>Cherry XX44</short>
      <flags>
        <!-- <flag>keypad</flag> -->
        <flag>keypad</flag>
      </flags>
    </reader>

  </readers>

</driver>

